<query id="getMusicList" action="select">
	<tables>
		<table name="musicbox_music" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="module_srl" var="module_srl" />
	</conditions>
	<navigation>
		<index var="sort_index" default="music_srl" order="desc" />
		<list_count var="list_count" default="20" />
		<page_count var="page_count" default="10" />
		<page var="page" default="1" />
	</navigation>
</query>
