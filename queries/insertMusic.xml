<query id="insertMusic" action="insert">
	<tables>
		<table name="musicbox_music" />
	</tables>
	<columns>
		<column name="music_srl" var="music_srl" notnull="notnull" default="sequence()" />
		<column name="module_srl" var="module_srl" notnull="notnull" />
		<column name="title" var="title" notnull="notnull" />
		<column name="artist" var="artist" notnull="notnull" />
		<column name="album" var="album" default="" />
		<column name="source_filename" var="source_filename" notnull="notnull" />
		<column name="uploaded_filename" var="uploaded_filename" notnull="notnull" />
		<column name="file_size" var="file_size" notnull="notnull" />
		<column name="comment" var="comment" default="" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>